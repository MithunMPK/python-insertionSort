def insertionSort(numbers):
    for i in range(1,len(numbers)):
        current_element = numbers[i]
        pos = i
        while current_element < numbers[pos-1] and pos>0:
            numbers[pos] = numbers[pos-1]
            pos = pos -1
        numbers[pos] = current_element
            

    
    
numbers = [8, 34, 2, 7, 8 ] 
insertionSort(numbers)
print(numbers)
